{
  "root": true,
  "overrides": [
    {
      "files": ["*.ts"],
      "extends": ["plugin:@angular-eslint/recommended", "airbnb-base"],
      "parserOptions": {
        "project": ["tsconfig.json"],
        "createDefaultProgram": true
      },
      "rules": {
        "import/prefer-default-export": "off",
        "class-methods-use-this": "off"
      }
    }
  ]
}
